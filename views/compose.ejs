          <form action="/compose" method="post">
            <h2>Compose</h2>
            <div class="form-group">
              <label>Title</label>
              <input type="text" name="newTitle" class="form-control" autocomplete="off">
            </div>
            <div class="form-group">
              <label>Author</label>
              <input type="text" name="authorName" class="form-control" autocomplete="off">
            </div>
            <div class="form-group">
              <label>Post</label><br>
              <textarea name="newPost" rows="7" cols="155" class="form-control"></textarea>
            </div>
            <button type="submit" class="btn btn-primary">Publish</button>
          </form>
        </div>
      </div>
    </div>
